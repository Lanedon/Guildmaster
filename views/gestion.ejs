<!DOCTYPE html>
<html>
<% if(role['role']!='anonyme'){ %>
    <head>
	<link rel="stylesheet" href="/styles.css" type="text/css">
        <title>Gestion</title>
        <style>
            a {text-decoration: none; color: black;}
        </style>
    </head>

    <body>
    <% include ./header.ejs %>
<br>

	<table>
		 <tr>
		  <th width="50px">id</th>
		  <th>pseudo</th>
		  <th>guilde</th>
		  <th>adresse email</th>
		  <th>role</th>
		  <th>modifier</th>
		  <th>bannir</th>
		  <th>supprimer</th>
		</tr>
		<% if(data.length){
		for(var i = 0;i < data.length;i++) { %>
		<tr>
		 <td><%=data[i].idUser%></td>
		 <td><%=data[i].pseudo%></td>
		 <td><%=data[i].nomGuilde%></td>
		 <td><%=data[i].mail%></td>
		 <td><%=data[i].role%></td>
 		 <td><a href="/guildmaster/gestion/modifier<%=data[i].idUser%>">modifier</a></td>
 		 <td><a href="/guildmaster/gestion/bannir<%=data[i].idUser%>">bannir</a></td>
		 <td><a href="/guildmaster/gestion/supprimer<%=data[i].idUser%>">supprimer</a></td>
		</tr>
		 <% } } %>
		 
	</table>
    </body>
<%  } else{ %>
<meta http-equiv="refresh" content="0; url=/STIFCO" />
<%  } %>
</html>
