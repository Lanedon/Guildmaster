<!DOCTYPE html>

<html>
    <head>

        <title>profil</title>
     <% include ./header.ejs %>
     <!-- Panel de prÃ©sentation -->
	<div class="container-fluid">
		<div class="row">
			<div class="col-sm-10 col-sm-offset-1">
				<div class="table-responsive">
				    <table class="table table-condensed table-stripped table-hover">
					    <tr class="thead">
					       <th>pseudo</th>
					       <th>guilde</th>
					       <th>adresse email</th>
					       <th>role</th>
					       <th>rang</th>
					       <th>prestige</th>
					       <th>or</th>
					    </tr>
					     <% if(data.length){
					     for(var i = 0;i < data.length;i++) { %>
					    <tr class="tbody text-center">
					      <td><%=data[i].login%></td>
					      <td><%=data[i].name%></td>
					      <td><%=data[i].email%></td>
					      <td><%=data[i].role%></td>
					      <td><%=data[i].rank%></td>
					      <td><%=data[i].prestige%></td>
					      <td><%=data[i].gold%></td>
					    </tr>
					      <% } } %>		      
					 </table>
				</div>
			</div>
	     </div>
	</div>
    <% include ./footer.ejs %>
    </body>
         <script type="text/javascript" src="/js/accueil.js"></script>
</html>
