    <div class="CSSTableGenerator" >
	<table>
	 <tr>
	  <th width="50px">id</th>
	  <th>nom</th>
	  <th>niveau</th>
	  <th>talent</th>
	  <th>prix</th>
	</tr>
	<% if(data.length){ 
	for(var i = 0;i < data.length;i++) { %>
	<tr>
	 <td><%=(i+1)%></td>
	 <td><%=data[i].surname%> <%=data[i].name%></td>
	 <td><%=data[i].level%></td>
	 <td><%=data[i].talent%></td>
	 <td><%=data[i].fee%></td>
	 </tr>
	<% }
	} %>
	
	</table>
    </div>
    
    </body>
